/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "paths": {
      "@angular/*": [ "../node_modules/@angular/*" ],
      "@coreui/angular-pro/*": ["../node_modules/@coreui/angular-pro/*"],
      "@docs-components/*": ["./src/components/*"]
    },
    "types": [],
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
